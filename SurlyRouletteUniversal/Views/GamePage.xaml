        </Viewbox>
        <Viewbox x:Name="ScoreViewbox" Grid.Column="1" HorizontalAlignment="Right" Margin="10,0,40,0">
			<StackPanel x:Name="ScoreBar" Grid.Row="0" Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right" FlowDirection="RightToLeft" Margin="0">
            
            <Grid x:Name="PayoutGrid" Style="{StaticResource ScoreBoxStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Rectangle Fill="Black" Style="{StaticResource ScoreBoxBGStyle}" />
                <TextBlock x:Name="textBlock2" Text="Payout" Margin="20,8" HorizontalAlignment="Center" FontSize="16"/>
                <TextBlock x:Name="PayoutAmount" Text="0" Grid.Row="1" Style="{StaticResource DigitalScoreTextStyle}" Foreground="{StaticResource chipColor}"/>
            </Grid>
            <Grid x:Name="TotalBetGrid" Style="{StaticResource ScoreBoxStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Rectangle Fill="Black" Style="{StaticResource ScoreBoxBGStyle}" />
                <TextBlock x:Name="textBlock1" Text="Total Bet" Margin="20,8" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="16"/>
                <TextBlock x:Name="TotalBetAmount" Text="0" Grid.Row="1" Style="{StaticResource DigitalScoreTextStyle}" Foreground="{StaticResource DigitalTextYellow}"/>
            </Grid>
            <Grid x:Name="WalletGrid" Style="{StaticResource ScoreBoxStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Rectangle Fill="Black" Style="{StaticResource ScoreBoxBGStyle}" />
                <TextBlock x:Name="textBlock" Text="Wallet" Margin="20,8" VerticalAlignment="Top" FontSize="16" HorizontalAlignment="Center"/>
                <TextBlock x:Name="WalletAmount" Text="0" Grid.Row="1" Style="{StaticResource DigitalScoreTextStyle}" Foreground="{StaticResource DigitalTextYellow}"/>
            </Grid>
            <Button x:Name="ReplenishButton" Grid.Row="1" Grid.Column="0" Content="Replenish" Click="ReplenishWallet_Click" Background="#99000000" Margin="20,0" HorizontalAlignment="Center"/>
        </StackPanel>
		</Viewbox>
        
        <Viewbox x:Name="WheelViewbox" Grid.Row="1" Grid.RowSpan="3" HorizontalAlignment="Right">
            <Canvas x:Name="canvas1" Width="640" Height="597">
                <Image x:Name="wheelBG" Source="ms-appx:///Assets/RouletteWheelBG.png" Style="{StaticResource WheelImageStyle}" />
                <Image x:Name="wheelCenter" Source="ms-appx:///Assets/RouletteWheelCenter.png" Style="{StaticResource WheelImageStyle}" RenderTransformOrigin="0.5,0.5">
                    <Image.RenderTransform>
                        <CompositeTransform Rotation="0"/>
                    </Image.RenderTransform>
                </Image>
                <Image x:Name="wheelTint" Source="ms-appx:///Assets/RouletteWheelTint.png" Style="{StaticResource WheelImageStyle}" Opacity="0.5"/>
                <Image x:Name="numberwang" Source="ms-appx:///Assets/RouletteSpinner.png" Style="{StaticResource WheelImageStyle}" RenderTransformOrigin="0.5,0.5">
                    <Image.RenderTransform>
                        <CompositeTransform Rotation="0"/>
                    </Image.RenderTransform>
                </Image>
                <Canvas x:Name="ballContainer" Width="1194" Height="1194" Canvas.Left="-470" Canvas.Top="50" RenderTransformOrigin="0.5,0.5">
                    <!--<Ellipse x:Name="ballShadow" Width="32" Height="32" Fill="Black" Canvas.Left="584" Canvas.Top="105" Opacity="0.4"/>-->
                    <Image x:Name="rouletteBall" Source="ms-appx:///Assets/RouletteBall.png" Canvas.Left="581" Canvas.Top="102" Width="35" Height="35"/>
                    <!--<Ellipse x:Name="DECOY" Width="32" Height="32" Fill="#FFF300FF" Canvas.Left="581" Canvas.Top="307"/>-->
                    <Canvas.RenderTransform>
                        <CompositeTransform Rotation="0"/>
                    </Canvas.RenderTransform>
                </Canvas>
            </Canvas>
        </Viewbox>


        <Viewbox x:Name="viewbox" Grid.Row="1" Grid.Column="2" Margin="0,0,40,0" >
            <!-- Wouldn't it be nice to bind these dimensions to RouletteTable's default -->
            <Canvas x:Name="canvas" Width="580" Height="305">
                <local:RouletteTable x:Name="tableControl">
                    <local:RouletteTable.RenderTransform>
                        <CompositeTransform/>
                    </local:RouletteTable.RenderTransform>
                </local:RouletteTable>
            </Canvas>
        </Viewbox>

        <!--</Grid>-->

        <!--Output grid for testing only-->
        <Grid x:Name="TestInfoUI" Grid.Column="0" Grid.ColumnSpan="1" Margin="10" HorizontalAlignment="Left" VerticalAlignment="Bottom" Grid.Row="1" Visibility="Collapsed">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
